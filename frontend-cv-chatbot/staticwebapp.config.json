{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "buildProperties": {
    "app_location": "/",
    "api_location": "",
    "output_location": "dist/frontend-cv-chatbot/browser"
  }
} 